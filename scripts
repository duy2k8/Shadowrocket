
var DIRECT = "DIRECT";
var PROXY = "PROXY 113.178.86.55:9000";
var keys = ["adguard", "shopee","elsa", "mitm.it", "adguard", "googlevideo", "youtube", "manulife", "revenuecat"];
function FindProxyForURL(url, host) {
  host = host.toLowerCase();
  for(var i = 0; i < keys.length; i++){
    var copyright = keys[i];
    if(shExpMatch(host, "*" + copyright + "*")){
      return PROXY;
    }
  }
  return DIRECT;
}
